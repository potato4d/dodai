{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "Node",
    "types": [
      "@types/node"
    ],
    "outDir": "./dist",
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
}
